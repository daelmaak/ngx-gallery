  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 demo",
    "serve": "http-server docs/",
    "build": "ng build",
    "build:demo": "ng build --project=demo --configuration production --base-href /ngx-doe-gallery/",
    "build:demo:local": "ng build --project=demo --configuration production --base-href /",
    "test": "ng test --project=gallery --browsers=ChromeHeadlessLocal",
    "test:demo": "ng test --project=demo",
    "test:ci": "npm test -- --watch=false --code-coverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "cd projects/gallery && standard-version -i ../../CHANGELOG.md && git push --follow-tags"
  },
  "private": true,
  "dependencies": {
